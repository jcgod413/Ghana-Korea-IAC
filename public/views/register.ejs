.alert{
	padding: 0;
	border: 0;
	margin: 0;
	color: red;
}
</style>
<script type="text/javascript">
	function go(){
		var fname_c=document.registerform.firstName.value;
		var lname_c=document.registerform.lastName.value;
		var tell_c=document.registerform.tellNo.value;
		var level_c=document.registerform.level.value;
		var st_c=document.registerform.status.value;
		var gen_c=document.registerform.inlineRadioOptions;
		var email_c=document.registerform.email.value;
		
		atpos=email_c.indexOf("@");
		dotpos=email_c.lastIndexOf(".");
		
		var pattern = /\s/g;
		
		//first name
		if(fname_c==""){
			document.getElementById("firstname_alert").innerHTML = "* please write first name";
			document.registerform.firstName.focus();
			return;
		}else{
			document.getElementById("firstname_alert").innerHTML = " ";
		}
		//last name
		if(lname_c==""){
			document.getElementById("lastname_alert").innerHTML = "* please write first name";
			document.registerform.lastName.focus();
			return;
		}else{
			document.getElementById("lastname_alert").innerHTML = " ";
		}
		//tell number
		if(tell_c==""){
			document.getElementById("tell_alert").innerHTML = "* please write tell number";
			document.registerform.tellNo.focus();
			return;
		}else{
			document.getElementById("tell_alert").innerHTML = " ";
		}
		//level
		if(level_c==""){
			document.getElementById("level_alert").innerHTML = "* please write level";
			document.registerform.level.focus();
			return;
		}else{
			document.getElementById("level_alert").innerHTML = " ";
		}	
		//status********************
		if(st_c=='none'){
			document.getElementById("status_alert").innerHTML = "* please select status";
			return;
		}else{
			document.getElementById("status_alert").innerHTML = " ";
		}
		//gender********************
		if(gen_c[0].checked==false && gen_c[1].checked==false){
			document.getElementById("gender_alert").innerHTML = "* please check gender";
			return;
		}else{
			document.getElementById("gender_alert").innerHTML = " ";
		}
		//email
		if(email_c==""){
			document.getElementById("email_alert").innerHTML = "* please write email";
			document.registerform.email.focus();
			return;
		}else if(atpos<1 || (dotpos-atpos <2)){
			document.getElementById("email_alert").innerHTML = "* please write correct email";
			document.registerform.email.focus();
			return;
		}else{
			document.getElementById("email_alert").innerHTML = " ";
		}
		document.registerform.submit();
	}
</script>

